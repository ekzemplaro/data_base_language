public sub Main()
	dim file_txt as string
	dim result as string
	dim Elt as String[]
	dim Sb as String
'
	print "*** 開始 ***"
'
	file_txt = Application.Args[1]
	print file_txt
	TRY result = file.Load (file_txt)
	Elt = Split (result,"\n")
	for each Sb in Elt
		if 0 < len (Sb) then
			print Sb
		endif
	next
'
	print "*** 終了 ***"
end
' ------------------------------------------------------------------
