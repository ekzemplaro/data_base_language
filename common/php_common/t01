$file = new SplFileObject($file_in); 
$file->setFlags(SplFileObject::READ_CSV);
foreach($file as $line)
	{
	if(!is_null($line[0]))
		{
		$dict_unit = array ();
		$dict_unit['name'] = $line[1];
		$dict_unit['population'] = $line[2];
		$dict_unit['date_mod'] = $line[3];
		$dict_aa[$line[0]]= $dict_unit;
		}
	}
$file = null;
