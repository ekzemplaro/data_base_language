class Sqlite3_read {
// -------------------------------------------------------------------
static function main()
{
	Sys.print("*** 開始 ***\n");

	var file_sqlite3 = Sys.args ()[0];

	Sys.println (file_sqlite3);

	var cnx = sys.db.Sqlite.open (file_sqlite3);

	Sql_manipulate.sql_display_proc (cnx);

	cnx.close();

	Sys.print("*** 終了 ***\n");
}
// -------------------------------------------------------------------
}
